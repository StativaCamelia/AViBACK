.header
    // NAV_BAR
    .nav
      .logoformobile
        a(href='http://localhost:5002/home')
          img(src='Images/logo.png' alt='AVi')
      .main_menu
        - let keyIds = {activeIds}
        - let valuesIds = JSON.parse(keyIds.activeIds)
        - let home = valuesIds[0]
        - let contact = valuesIds[1]
        - let documentation = valuesIds[2]
        - let profile = valuesIds[3]
        - let logout = valuesIds[4]
        - let map = valuesIds[5]
        - let pie = valuesIds[6]
        - let barChart = valuesIds[7]
        - let lineChart = valuesIds[8]
        ul
          li
            a.logo(href='http://localhost:5002/home')
              img.effect3(src='Images/logo_renew.png' alt='AVi')
          li
            a.myButton(id=home href='http://localhost:5002/home') HOME
          li
            a.myButton(id=contact href='http://localhost:5002/contact') CONTACT
          li
            a.myButton(id=documentation href='http://localhost:5002/documentation') DOCUMENTATION
          li
            a.myButton#button(class=profile)
          li
            a.myButton#logout(class=logout) LOGOUT
      .statistics
        ul
          li.effect1
            a(id=map href='http://localhost:5002/map')
              img.for_img(title='Maps' src='./Images/Icons/map.png')
            a(id=pie href='http://localhost:5002/pie')
              img.for_img(title='Pie Chart' src='./Images/Icons/pie-white.png')
            a(id=barChart href='http://localhost:5002/chart')
              img.for_img(title='Chart' src='./Images/Icons/bar-chart.png')
            a(id=lineChart href='http://localhost:5002/line')
              img.for_img(title='Line Chart' src='./Images/Icons/line-chart.png')